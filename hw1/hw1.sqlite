# Q1
SELECT DISTINCT DraftFrom 
FROM Draft 
WHERE FirstName = "Lew" AND LastName = "Alcindor";

#Q2
SELECT DISTINCT T.Name 
FROM Teams AS T, Players AS P 
WHERE P.TeamID = T.TeamID AND P.PlayerID = "abdulka01";

#Q3
SELECT COUNT(*) FROM 
	(SELECT TeamID 
	 FROM Players 
	 WHERE PlayerID = "abdulka01" 
	 GROUP BY TeamID);

